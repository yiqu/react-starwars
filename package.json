    "firebase": "^9.10.0",
    "formik": "^2.2.9",
    "gh-pages": "^4.0.0",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.0.3",
    "react-moment": "^1.1.2",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-use": "^17.4.0",
    "rxjs": "^7.5.6",
    "swr": "^2.0.0",
    "typescript": "^4.8.4",
